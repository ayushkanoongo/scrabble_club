<div class="nested-fields">
  <%= f.hidden_field :_destroy %>
  <div>
    <%= f.label :member %>
    <%= f.collection_select(:member_id, Member.all, :id, :first_name, { :prompt => 'Select Member'}, { class: 'form-control' }) %>
  </div>
  <div>
    <%= f.label :score %>
    <%= f.number_field :score, in: 1.0..100.0, step: 0.5, class: 'form-control' %>
  </div>
  <div>
    <%= link_to "Remove", '#', class: "remove_fields" %>
  </div>
</div>